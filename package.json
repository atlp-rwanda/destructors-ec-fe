    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-plugin-jest-dom": "^4.0.3",
